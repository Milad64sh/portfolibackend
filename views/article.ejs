<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Article</title>
  </head>
  <body>
    <header>
      <nav>
        <ul>
          <li><a href="/">article</a></li>
          <li>
            <a href="/admin/add-article">Add Article</a>
          </li>
        </ul>
      </nav>
    </header>
    <main>
      <%if (articles.length > 0) {%>
      <div>
        <% for(let article of articles) { %>
        <h1><%= article.title %></h1>
        <p><%= article.content %></p>
        <% } %>
      </div>
      <% } %>
    </main>
  </body>
</html>
